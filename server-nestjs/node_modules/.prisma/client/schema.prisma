generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_user {
  userId       Int      @id @default(autoincrement())
  username     String
  password     String
  user_email   String
  user_phone   Int
  user_address String
  user_birtday DateTime
  create_date  DateTime
  update_date  DateTime
}
